    "@google-cloud/storage": "^5.14.8",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.4.6",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-random-string": "^3.3.0",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.1.6",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.8",
    "firebase-admin": "^9.12.0",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.1.3",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^4.1.6",
