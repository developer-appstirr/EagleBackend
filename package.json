{
  "name": "eagle-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc -w & cross-env NODE_ENV=development DEBUG=node-server:* nodemon src/index.ts",
    "dev:debug": "tsc -w & cross-env NODE_ENV=development DEBUG=node-server:* nodemon --inspect=0.0.0.0 src/index.ts",
    "start": "node dist/index.js",
    "production": "cross-env NODE_ENV=production node dist/index.js",
    "production:debug": "cross-env NODE_ENV=production DEBUG=node-server:* node dist/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "entity:create": "npm run typeorm entity:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- --n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "seed:create": "npm run typeorm migration:create -- -d src/migrations/seeds -n",
    "seed:config": "npm run config --connection default",
    "seed:run": "npm run seed --connection default"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
